<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Calculator">

    <TextView
        android:id="@+id/text_Damage_Low_Roll"
        android:layout_width="125dp"
        android:layout_height="24dp"
        android:layout_marginTop="5dp"
        android:background="@drawable/my_border"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/text_Damage_Low_Roll_Label" />

    <TextView
        android:id="@+id/text_Damage_Low_Roll_Label"
        android:layout_width="125dp"
        android:layout_height="18sp"
        android:layout_marginTop="10dp"
        android:text="Low"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/text_Damage_Average_Roll"
        android:layout_width="125dp"
        android:layout_height="24dp"
        android:layout_marginTop="5dp"
        android:background="@drawable/my_border"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.8"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/text_Damage_Average_Roll_Label" />

    <TextView
        android:id="@+id/text_Damage_Average_Roll_Label"
        android:layout_width="125dp"
        android:layout_height="18dp"
        android:layout_marginTop="8dp"
        android:text="Average"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.8"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <TextView
        android:id="@+id/text_Damage_High_Roll"
        android:layout_width="125dp"
        android:layout_height="24dp"
        android:layout_marginTop="5dp"
        android:background="@drawable/my_border"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/text_Damage_High_Roll_Label" />

    <TextView
        android:id="@+id/text_Damage_High_Roll_Label"
        android:layout_width="125dp"
        android:layout_height="20dp"
        android:layout_marginTop="10dp"
        android:text="High"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/text_Damage_Low_Roll" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Projected Damage"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.204"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewServantName"
        app:layout_constraintVertical_bias="0.075" />

    <Button
        android:id="@+id/button_Calculate_Damage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Calculate"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editText_DMGPlus_Buff" />

    <EditText
        android:id="@+id/editText_Card_Buff"
        android:layout_width="125dp"
        android:layout_height="48dp"
        android:layout_marginBottom="50dp"
        android:autofillHints=""
        android:ems="10"
        android:inputType="number"
        android:textSize="18sp"
        app:boxStrokeWidth="0dp"
        app:boxStrokeWidthFocused="0dp"
        app:layout_constraintBottom_toTopOf="@+id/editText_NPDamage_Buff"
        app:layout_constraintEnd_toStartOf="@+id/editText_Attack_Buff"
        app:layout_constraintHorizontal_bias="0.8"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/text_Damage_High_Roll"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/text_Card_Buffs"
        android:layout_width="125sp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="5dp"
        android:text="Card Buffs"
        android:textSize="14sp"
        app:layout_constraintBottom_toTopOf="@+id/editText_Card_Buff"
        app:layout_constraintEnd_toStartOf="@+id/text_Attack_Buff"
        app:layout_constraintHorizontal_bias="0.8"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/text_Damage_High_Roll"
        app:layout_constraintVertical_bias="1.0" />

    <EditText
        android:id="@+id/editText_SE_Buff"
        android:layout_width="125dp"
        android:layout_height="48dp"
        android:autofillHints=""
        android:ems="10"
        android:inputType="number"
        android:textSize="18sp"
        app:boxStrokeWidth="0dp"
        app:boxStrokeWidthFocused="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/editText_DMGPlus_Buff"
        app:layout_constraintHorizontal_bias="0.8"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.8" />

    <TextView
        android:id="@+id/text_SE_Buff"
        android:layout_width="125dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="5dp"
        android:text="NP Extra Damage"
        android:textSize="14sp"
        app:layout_constraintBottom_toTopOf="@+id/editText_SE_Buff"
        app:layout_constraintEnd_toStartOf="@+id/text_DMGPlus_Buff"
        app:layout_constraintHorizontal_bias="0.8"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <EditText
        android:id="@+id/editText_Attack_Buff"
        android:layout_width="125dp"
        android:layout_height="48dp"
        android:layout_marginBottom="50dp"
        android:autofillHints=""
        android:ems="10"
        android:inputType="number"
        app:boxStrokeWidth="0dp"
        app:boxStrokeWidthFocused="0dp"
        app:layout_constraintBottom_toTopOf="@+id/editText_Pmod_Buff"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.75"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/text_Damage_High_Roll"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/text_Attack_Buff"
        android:layout_width="125dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="5dp"
        android:text="Attack Buffs"
        android:textSize="14sp"
        app:layout_constraintBottom_toTopOf="@+id/editText_Attack_Buff"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.75"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/text_Damage_High_Roll"
        app:layout_constraintVertical_bias="1.0" />

    <EditText
        android:id="@+id/editText_NPDamage_Buff"
        android:layout_width="125dp"
        android:layout_height="48dp"
        android:layout_marginBottom="50dp"
        android:autofillHints=""
        android:ems="10"
        android:inputType="number"
        app:boxStrokeWidth="0dp"
        app:boxStrokeWidthFocused="0dp"
        app:layout_constraintBottom_toTopOf="@+id/editText_SE_Buff"
        app:layout_constraintEnd_toStartOf="@+id/editText_Pmod_Buff"
        app:layout_constraintHorizontal_bias="0.8"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/text_NPDamage_Buff"
        android:layout_width="125dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="5dp"
        android:text="NP Damage Buffs"
        android:textSize="14sp"
        app:layout_constraintBottom_toTopOf="@+id/editText_NPDamage_Buff"
        app:layout_constraintEnd_toStartOf="@+id/text_Pmod_Buff"
        app:layout_constraintHorizontal_bias="0.8"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <EditText
        android:id="@+id/editText_Pmod_Buff"
        android:layout_width="125dp"
        android:layout_height="48dp"
        android:layout_marginBottom="50dp"
        android:autofillHints=""
        android:ems="10"
        android:inputType="number"
        app:boxStrokeWidth="0dp"
        app:boxStrokeWidthFocused="0dp"
        app:layout_constraintBottom_toTopOf="@+id/editText_DMGPlus_Buff"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.75"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/text_Pmod_Buff"
        android:layout_width="125dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="5dp"
        android:text="PowerMod Buffs"
        android:textSize="14sp"
        app:layout_constraintBottom_toTopOf="@+id/editText_Pmod_Buff"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.75"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/text_DMGPlus_Buff"
        android:layout_width="125dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="5dp"
        android:text="Flat Damage Buffs"
        android:textSize="14sp"
        app:layout_constraintBottom_toTopOf="@+id/editText_DMGPlus_Buff"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.75"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <EditText
        android:id="@+id/editText_DMGPlus_Buff"
        android:layout_width="125dp"
        android:layout_height="48dp"
        android:autofillHints=""
        android:ems="10"
        android:inputType="number"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.75"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.8" />

    <AutoCompleteTextView
        android:id="@+id/autoCompleteServantName"
        android:layout_width="200sp"
        android:layout_height="24sp"
        android:background="@null"
        android:minHeight="48dp"
        app:layout_constraintBottom_toTopOf="@+id/text_Attack_Buff"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.27"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewServantName"
        app:layout_constraintVertical_bias="0.053" />

    <TextView
        android:id="@+id/textViewServantName"
        android:layout_width="125sp"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="Name"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Spinner
        android:id="@+id/spinnerLevel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:dropDownWidth="match_parent"
        android:minHeight="48dp"
        android:spinnerMode="dropdown"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.594"
        app:layout_constraintStart_toEndOf="@+id/textViewLevel"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textViewLevel"
        android:layout_width="30sp"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="Level"
        android:textSize="12sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.226"
        app:layout_constraintStart_toEndOf="@+id/textViewServantName"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textViewAttackStat"
        android:layout_width="30sp"
        android:layout_height="wrap_content"
        android:layout_marginTop="52dp"
        android:text="ATK"
        android:textSize="12sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.271"
        app:layout_constraintStart_toEndOf="@+id/textViewServantName"
        app:layout_constraintTop_toBottomOf="@+id/textViewLevel" />

    <TextView
        android:id="@+id/textViewAtkStat"
        android:layout_width="80sp"
        android:layout_height="21dp"
        android:layout_marginTop="52dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.423"
        app:layout_constraintStart_toEndOf="@+id/textViewAttackStat"
        app:layout_constraintTop_toBottomOf="@+id/textViewLevel" />

</androidx.constraintlayout.widget.ConstraintLayout>